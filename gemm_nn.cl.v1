__kernel void
gemm_nn(int m, int n, int k, float alpha, __global float *A, int lda,
        __global float *B, int ldb, __global float *C, int ldc)
{	
	int i = get_global_id(0);
	int j = get_global_id(1);
	if (j >= 0 && j < n && i >= 0 && i < m) {
		for (int l = 0; l < k; ++l) {
			C[i * ldc + j] += alpha * A[i * lda + l] * B[l * ldb + j];
		}
	}
}